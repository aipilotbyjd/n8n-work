{
  "name": "@n8n-work/node-sdk",
  "version": "1.0.0",
  "description": "SDK for developing custom nodes for the n8n-work workflow platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "n8n-work": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/cli/index.js",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/cli/index.ts",
    "dev:server": "ts-node src/cli/index.ts dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:advanced": "ts-node src/cli/index.ts test",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "docs:build": "typedoc src/index.ts --out docs",
    "docs:serve": "http-server docs -p 8080",
    "generate": "ts-node src/cli/index.ts generate",
    "validate": "ts-node src/cli/index.ts validate",
    "analyze": "ts-node src/cli/index.ts analyze"
  },
  "keywords": [
    "n8n-work",
    "workflow",
    "automation",
    "node",
    "sdk",
    "plugin",
    "typescript",
    "microservices",
    "low-code",
    "integration"
  ],
  "author": "N8N-Work Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/n8n-work/node-sdk-js.git"
  },
  "bugs": {
    "url": "https://github.com/n8n-work/node-sdk-js/issues"
  },
  "homepage": "https://github.com/n8n-work/node-sdk-js#readme",
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "ajv": "^8.12.0",
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "inquirer": "^8.2.6",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "mustache": "^4.2.0",
    "ora": "^5.4.1",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "ws": "^8.14.2",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.8",
    "@types/lodash": "^4.14.202",
    "@types/mustache": "^4.2.5",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.4",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "typescript": ">=4.9.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
