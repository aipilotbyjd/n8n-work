{
  "dashboard": {
    "id": null,
    "title": "N8N Work - Security Monitoring",
    "tags": ["n8n-work", "security", "sandbox"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "hideControls": false,
    "links": [],
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Security Violations (Last 24h)",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(n8n_sandbox_security_violations_total[24h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 10
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Failed Authentication Attempts",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(n8n_auth_failures_total[1h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "red",
                  "value": 20
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Blocked Network Requests",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(increase(n8n_network_requests_blocked_total[1h]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 50
                },
                {
                  "color": "red",
                  "value": 200
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Sandbox Violations by Type",
        "type": "piechart",
        "targets": [
          {
            "expr": "sum by (violation_type) (increase(n8n_sandbox_security_violations_total[24h]))",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Security Event Timeline",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(n8n_sandbox_security_violations_total[5m])",
            "legendFormat": "Security Violations/min",
            "refId": "A"
          },
          {
            "expr": "rate(n8n_auth_failures_total[5m])",
            "legendFormat": "Auth Failures/min",
            "refId": "B"
          },
          {
            "expr": "rate(n8n_network_requests_blocked_total[5m])",
            "legendFormat": "Blocked Requests/min",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 24,
          "x": 0,
          "y": 8
        },
        "yAxes": [
          {
            "label": "Events/min",
            "min": 0,
            "show": true
          },
          {
            "show": true
          }
        ]
      },
      {
        "id": 6,
        "title": "Sandbox Resource Limits Exceeded",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (resource_type) (rate(n8n_sandbox_resource_limit_exceeded_total[5m]))",
            "legendFormat": "{{ resource_type }}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 17
        },
        "yAxes": [
          {
            "label": "Violations/min",
            "min": 0,
            "show": true
          },
          {
            "show": true
          }
        ]
      },
      {
        "id": 7,
        "title": "Network Traffic Analysis",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(n8n_network_requests_total[5m]))",
            "legendFormat": "Total Requests/min",
            "refId": "A"
          },
          {
            "expr": "sum(rate(n8n_network_requests_allowed_total[5m]))",
            "legendFormat": "Allowed Requests/min",
            "refId": "B"
          },
          {
            "expr": "sum(rate(n8n_network_requests_blocked_total[5m]))",
            "legendFormat": "Blocked Requests/min",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 17
        },
        "yAxes": [
          {
            "label": "Requests/min",
            "min": 0,
            "show": true
          },
          {
            "show": true
          }
        ]
      },
      {
        "id": 8,
        "title": "Security Event Details",
        "type": "table",
        "targets": [
          {
            "expr": "topk(10, sum by (violation_type, source_ip, user_id) (increase(n8n_sandbox_security_violations_total[1h])))",
            "format": "table",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 10,
          "w": 24,
          "x": 0,
          "y": 26
        }
      },
      {
        "id": 9,
        "title": "Sandbox Health Status",
        "type": "table",
        "targets": [
          {
            "expr": "n8n_sandbox_instances_active",
            "format": "table",
            "refId": "A"
          },
          {
            "expr": "n8n_sandbox_instances_failed",
            "format": "table",
            "refId": "B"
          },
          {
            "expr": "n8n_sandbox_cpu_usage_percent",
            "format": "table",
            "refId": "C"
          },
          {
            "expr": "n8n_sandbox_memory_usage_bytes / 1024 / 1024",
            "format": "table",
            "refId": "D"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 36
        }
      }
    ],
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    }
  }
}