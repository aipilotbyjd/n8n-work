# Application Configuration
NODE_ENV=development
LOG_LEVEL=info
PORT=3000

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=
REDIS_DB=0

# RabbitMQ Configuration
RABBITMQ_URL=amqp://guest:guest@localhost:5672
RABBITMQ_EXCHANGE=n8n_work_steps
RABBITMQ_QUEUE=node_runner_queue
RABBITMQ_ROUTING_KEY=node.execute
RABBITMQ_RESULTS_EXCHANGE=n8n_work_results
RABBITMQ_RESULTS_ROUTING_KEY=execution.result
RABBITMQ_PREFETCH_COUNT=10

# OpenTelemetry Configuration
OTEL_SERVICE_NAME=n8n-work-node-runner
OTEL_SERVICE_VERSION=1.0.0
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4318/v1/traces
OTEL_EXPORTER_OTLP_HEADERS=
OTEL_RESOURCE_ATTRIBUTES=

# Sandbox Configuration
SANDBOX_STRATEGY=vm2
MAX_EXECUTION_TIME=30000
MAX_MEMORY_USAGE=512
SANDBOX_TIMEOUT=45000

# Security Configuration
ALLOW_UNSAFE_EVAL=false
ALLOW_CHILD_PROCESS=false
ALLOWED_MODULES=crypto,url,querystring,util

# API Configuration
API_RATE_LIMIT_MAX=100
API_RATE_LIMIT_TIMEWINDOW=60000
CORS_ORIGIN=*
ENABLE_SWAGGER=true

# Health Check Configuration
HEALTH_CHECK_TIMEOUT=5000
HEALTH_CHECK_INTERVAL=30000

# Metrics Configuration
METRICS_ENABLED=true
METRICS_PREFIX=n8n_work_node_runner

# Built-in Nodes Configuration
HTTP_REQUEST_TIMEOUT=30000
HTTP_REQUEST_MAX_REDIRECTS=5
WEBHOOK_RETRY_ATTEMPTS=3
SLACK_API_BASE_URL=https://slack.com/api
