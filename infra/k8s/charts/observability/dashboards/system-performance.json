{
  "dashboard": {
    "id": null,
    "title": "N8N Work - System Performance",
    "tags": ["n8n-work", "system", "performance"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "gnetId": null,
    "graphTooltip": 0,
    "hideControls": false,
    "links": [],
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "CPU Usage by Component",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(container_cpu_usage_seconds_total{pod=~\"orchestrator-.*\"}[5m])) by (pod) * 100",
            "legendFormat": "Orchestrator - {{ pod }}",
            "refId": "A"
          },
          {
            "expr": "sum(rate(container_cpu_usage_seconds_total{pod=~\"engine-go-.*\"}[5m])) by (pod) * 100",
            "legendFormat": "Engine-Go - {{ pod }}",
            "refId": "B"
          },
          {
            "expr": "sum(rate(container_cpu_usage_seconds_total{pod=~\"node-runner-js-.*\"}[5m])) by (pod) * 100",
            "legendFormat": "Node-Runner - {{ pod }}",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "yAxes": [
          {
            "label": "CPU Usage %",
            "max": 100,
            "min": 0,
            "show": true
          },
          {
            "show": true
          }
        ]
      },
      {
        "id": 2,
        "title": "Memory Usage by Component",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(container_memory_working_set_bytes{pod=~\"orchestrator-.*\"}) by (pod) / 1024 / 1024",
            "legendFormat": "Orchestrator - {{ pod }}",
            "refId": "A"
          },
          {
            "expr": "sum(container_memory_working_set_bytes{pod=~\"engine-go-.*\"}) by (pod) / 1024 / 1024",
            "legendFormat": "Engine-Go - {{ pod }}",
            "refId": "B"
          },
          {
            "expr": "sum(container_memory_working_set_bytes{pod=~\"node-runner-js-.*\"}) by (pod) / 1024 / 1024",
            "legendFormat": "Node-Runner - {{ pod }}",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "yAxes": [
          {
            "label": "Memory Usage (MB)",
            "min": 0,
            "show": true
          },
          {
            "show": true
          }
        ]
      },
      {
        "id": 3,
        "title": "Network I/O",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(container_network_receive_bytes_total{pod=~\"orchestrator-.*|engine-go-.*|node-runner-js-.*\"}[5m])) by (pod) / 1024",
            "legendFormat": "RX - {{ pod }}",
            "refId": "A"
          },
          {
            "expr": "sum(rate(container_network_transmit_bytes_total{pod=~\"orchestrator-.*|engine-go-.*|node-runner-js-.*\"}[5m])) by (pod) / 1024",
            "legendFormat": "TX - {{ pod }}",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 9
        },
        "yAxes": [
          {
            "label": "Network I/O (KB/s)",
            "min": 0,
            "show": true
          },
          {
            "show": true
          }
        ]
      },
      {
        "id": 4,
        "title": "Pod Restart Count",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(increase(kube_pod_container_status_restarts_total{pod=~\"orchestrator-.*|engine-go-.*|node-runner-js-.*\"}[1h])) by (pod)",
            "legendFormat": "{{ pod }}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 9
        },
        "yAxes": [
          {
            "label": "Restart Count",
            "min": 0,
            "show": true
          },
          {
            "show": true
          }
        ]
      },
      {
        "id": 5,
        "title": "Database Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(postgresql_queries_total[5m])",
            "legendFormat": "Queries/sec",
            "refId": "A"
          },
          {
            "expr": "avg(postgresql_query_duration_seconds)",
            "legendFormat": "Avg Query Duration",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 0,
          "y": 18
        },
        "yAxes": [
          {
            "label": "Queries/sec",
            "show": true
          },
          {
            "label": "Duration (seconds)",
            "show": true
          }
        ]
      },
      {
        "id": 6,
        "title": "Message Queue Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rabbitmq_queue_messages)",
            "legendFormat": "Queue Length",
            "refId": "A"
          },
          {
            "expr": "rate(rabbitmq_queue_messages_published_total[5m])",
            "legendFormat": "Messages Published/sec",
            "refId": "B"
          },
          {
            "expr": "rate(rabbitmq_queue_messages_delivered_total[5m])",
            "legendFormat": "Messages Delivered/sec",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 9,
          "w": 12,
          "x": 12,
          "y": 18
        },
        "yAxes": [
          {
            "label": "Messages",
            "show": true
          },
          {
            "label": "Messages/sec",
            "show": true
          }
        ]
      },
      {
        "id": 7,
        "title": "Sandbox Resource Usage",
        "type": "table",
        "targets": [
          {
            "expr": "sum by (instance) (n8n_sandbox_cpu_usage_percent)",
            "format": "table",
            "refId": "A"
          },
          {
            "expr": "sum by (instance) (n8n_sandbox_memory_usage_bytes) / 1024 / 1024",
            "format": "table",
            "refId": "B"
          },
          {
            "expr": "sum by (instance) (n8n_sandbox_active_instances)",
            "format": "table",
            "refId": "C"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 27
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": [
        "5s",
        "10s",
        "30s",
        "1m",
        "5m",
        "15m",
        "30m",
        "1h",
        "2h",
        "1d"
      ]
    }
  }
}